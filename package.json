{
  "name": "node-express-skeleton",
  "version": "1.0.0",
  "description": "Esqueleto de nodeJs que implementa arquitectura hexagonal con principios SOLID",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon",
    "migrate:init": "ts-node migrations/index.ts init",
    "migrate:create": "ts-node migrations/index.ts new -n",
    "migrate:up": "ts-node migrations/index.ts up",
    "migrate:down_last": "ts-node migrations/index.ts down --last",
    "migrate:status": "ts-node migrations/index.ts status",
    "migrate:down_all": "ts-node migrations/index.ts down --all"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.possumus.tech/skeletons/nodejs/nodejs-skeleton.git"
  },
  "keywords": [
    "clean-code"
  ],
  "author": "Guillermo Morton",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.8",
    "@types/sinon": "^17.0.3",
    "@types/sinon-express-mock": "^1.3.12",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.7.3",
    "mongo-migrate-ts": "^1.6.2",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.7",
    "sinon": "^19.0.2",
    "sinon-express-mock": "^2.2.1",
    "ts-sinon": "^2.0.2"
  }
}
